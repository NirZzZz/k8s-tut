apiVersion: apps/v1
kind: Deployment
metadata:
  name: superset-deployment
spec:
  replicas: 1
  selector:
    matchLabels:
      app: superset
  template:
    metadata:
      labels:
        app: superset
    spec:
      containers:
      - name: superset-container
        image: amancevice/superset:latest
        ports:
        - containerPort: 80
---
apiVersion: v1
kind: Service
metadata:
  name: superset-service
spec:
  selector:
    app: superset
  ports:
  - protocol: TCP
    port: 80
    targetPort: 80
  type: ClusterIP
